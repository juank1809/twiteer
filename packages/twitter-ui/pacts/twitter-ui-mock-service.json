{
  "consumer": {
    "name": "twitter-ui"
  },
  "interactions": [
    {
      "description": "favorite tweet",
      "providerState": "favoriting a tweet tweet",
      "request": {
        "method": "POST",
        "path": "/tweets/favorite/1"
      },
      "response": {
        "body": true,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 200
      }
    },
    {
      "description": "post one tweet",
      "providerState": "post tweet",
      "request": {
        "body": {
          "tweet": {
            "favoriteCount": 3,
            "id": 1,
            "message": "I like surfing",
            "replyCount": 2,
            "retweetCount": 1,
            "type": "default",
            "user": {
              "fullName": "Christoper Francisco",
              "username": "@christopher"
            }
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/tweets"
      },
      "response": {
        "body": [
          {
            "favoriteCount": 3,
            "id": 1,
            "message": "I like surfing",
            "replyCount": 2,
            "retweetCount": 1,
            "type": "default",
            "user": {
              "fullName": "Christoper Francisco",
              "username": "@christopher"
            }
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "retweet one tweet",
      "providerState": "retweet tweet",
      "request": {
        "method": "POST",
        "path": "/tweets/retweet/1"
      },
      "response": {
        "body": true,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 200
      }
    },
    {
      "description": "get all tweets",
      "providerState": "tweets exist",
      "request": {
        "method": "GET",
        "path": "/tweets"
      },
      "response": {
        "body": [
          {
            "favoriteCount": 3,
            "id": 1,
            "message": "I like surfing",
            "replyCount": 2,
            "retweetCount": 1,
            "type": "default",
            "user": {
              "fullName": "Christoper Francisco",
              "username": "@christopher"
            }
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.2.2"
    },
    "pactRust": {
      "ffi": "0.3.14",
      "models": "1.0.0"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "mock-service"
  }
}